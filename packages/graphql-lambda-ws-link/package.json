{
  "name": "graphql-lambda-ws-link",
  "version": "0.0.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JetClosing/graphql-lambda-websocket.git",
    "directory": "packages/graphql-lambda-ws-link"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn build:babel && yarn build:flow",
