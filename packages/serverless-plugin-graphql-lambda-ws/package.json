    "pascal-case": "^2.0.1",
    "serverless-plugin-custom-roles": "1.1.0",
    "serverless-plugin-tables": "1.0.1"
  },
  "peerDependencies": {
    "graphql-lambda-ws": "*",
    "serverless": "^1.39.0"
  }
}
