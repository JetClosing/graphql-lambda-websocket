  "main": "lib/index.js",
  "files": [
    "layers",
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn build:babel && yarn build:flow",
    "build:babel": "npx babel src --config-file ../../babel.config.js --ignore '**/__tests__/**' --out-dir lib",
    "build:flow": "npx flow-copy-source -i '**/__tests__/**' src lib"
  },
  "dependencies": {
